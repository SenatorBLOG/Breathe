{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },  
    { "src": "package.json", "use": "@vercel/node" }, 
    { "src": "api/**/*.js", "use": "@vercel/node" } 
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/server.js" }, 
    { "src": "/(.*)", "dest": "/" }  
  ]
}